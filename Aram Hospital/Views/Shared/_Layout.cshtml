<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Aram Hospital</title>
    @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    

    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <meta name="copyright" content="MACode ID, https://macodeid.com/">

    <link rel="stylesheet" href="~/Content/assets/css/maicons.css">

    <link rel="stylesheet" href="~/Content/css/bootstrap.css">

    <link rel="stylesheet" href="~/Content/assets/vendor/owl-carousel/css/owl.carousel.css">

    <link rel="stylesheet" href="~/Content/assets/vendor/animate/animate.css">

    <link rel="stylesheet" href="~/Content/assets/css/theme.css">

</head>
<body>
    <div class="back-to-top"></div>

    <header>
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8 text-sm">
                        <div class="site-info">
                            <a href="#"><span class="mai-call text-primary"></span> 9444716971</a>
                            <span class="divider">|</span>
                            <a href="~/Home/Index"><span class="mai-mail text-primary"></span> Aramhospital@gmail.com</a>
                        </div>
                    </div>
                    <div class="col-sm-4 text-right text-sm">
                        <div class="social-mini-button">
                            <a href="#"><span class="mai-logo-facebook-f"></span></a>
                            <a href="#"><span class="mai-logo-twitter"></span></a>
                            <a href="#"><span class="mai-logo-dribbble"></span></a>
                            <a href="#"><span class="mai-logo-instagram"></span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <nav class="navbar navbar-expand-lg navbar-light shadow-sm">
            <div class="container">
                <a class="navbar-brand" href="#"><span class="text-primary">Aram</span>-Hospital</a>



                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupport">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="~/Home/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/About">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/Docters">Doctors</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/News">News</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/contact">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a class="btn btn-primary ml-lg-3" href="~/Admin/AdminSignin">Admin Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container body-content">
        @RenderBody()@* This Render in html *@
        <hr />

    </div>
   


    <footer class="page-footer">
        <center>
            <div class="col-sm-6 col-lg-3 py-3">
                <h5>Contact</h5>
                <p class="footer-link mt-2">
                    349, Thiruvottriyur High Road,
                    Thiruvottriyur,
                    Chennai 600019
                    Tamil Nadu
                </p>
                <a href="#" class="footer-link">Ph: 044 4050 6070</a>
                <a href="#" class="footer-link">AramHospital@gmail.com</a>

                <h5 class="mt-3">Social Media</h5>
                <div class="footer-sosmed mt-3">
                    <a href="#" target="_blank"><span class="mai-logo-facebook-f"></span></a>
                    <a href="#" target="_blank"><span class="mai-logo-twitter"></span></a>
                    <a href="#" target="_blank"><span class="mai-logo-google-plus-g"></span></a>
                    <a href="#" target="_blank"><span class="mai-logo-instagram"></span></a>
                    <a href="#" target="_blank"><span class="mai-logo-linkedin"></span></a>
                </div>
            </div>
            
        </center>

        <hr>


        
        
    </footer>

    <script src="~/Content/assets/js/jquery-3.5.1.min.js"></script>

    <script src="~/Content/assets/js/bootstrap.bundle.min.js"></script>

    <script src="~/Content/assets/vendor/owl-carousel/js/owl.carousel.min.js"></script>

    <script src="~/Content/assets/vendor/wow/wow.min.js"></script>

    <script src="~/Content/assets/js/theme.js"></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
